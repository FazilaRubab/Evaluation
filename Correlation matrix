import pandas as pd
data=pd.read_csv("arg_busan.csv")
data.head()

import matplotlib.pyplot as plt
import numpy as np
x = data["tide_cm"]
y =data["water_temp-c"]
plt.plot(x, y, 'o')
plt.title("tide_cm vs water_temp_c")
plt.xlabel("tide_cm")
plt.ylabel("water_temp_c")
m, b = np.polyfit(x, y, 1)
plt.plot(x, m*x + b)
plt.savefig('tide vs water_temp.png')

import matplotlib.pyplot as plt
import numpy as np
x = data["tide_cm"]
y =data["sal_psu"]
plt.title("tide_cm vs sal_psu")
plt.xlabel("tide_cm")
plt.ylabel("sal_psu")
plt.plot(x, y, 'o')
m, b = np.polyfit(x, y, 1)
plt.plot(x, m*x + b)
plt.savefig('tide vs sal_psu.png')






import pandas as pd

df = pd.read_csv("arg_busan.csv")


from fast_ml.model_development import train_valid_test_split

X_train, y_train, X_valid, y_valid, X_test, y_test = train_valid_test_split(df, target = 'SalePrice', 
                                                                            method='sorted', sort_by_col='saledate',
                                                                            train_size=0.8, valid_size=0.1, test_size=0.1)

print(X_train.shape), print(y_train.shape)
print(X_valid.shape), print(y_valid.shape)
print(X_test.shape), print(y_test.shape)




